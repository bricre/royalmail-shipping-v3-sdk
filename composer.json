{
  "name": "bricre/royalmail-shipping-v3-sdk",
  "description": "Royal Mail Shipping V3 REST API SDK generated from swagger specification",
  "keywords": [
    "RoyalMail",
    "Royal Mail",
    "Shipping",
    "SDK",
    "API",
    "REST"
  ],
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Allan Sun",
      "email": "allan.sun@bricre.com"
    }
  ],
  "require": {
    "php": ">=7.4|^8.0",
    "allansun/openapi-runtime": "^2.1"
  },
  "require-dev": {
    "allansun/openapi-code-generator": "^0.2.3",
    "phpunit/phpunit": "^9.5.4",
    "guzzlehttp/guzzle": "^v7.3.0",
    "phpspec/prophecy": "^1.12",
    "phpspec/prophecy-phpunit": "^2.0",
    "monolog/monolog": "^2.2.0"
  },
  "autoload": {
    "psr-4": {
      "RoyalMail\\SDK\\V3Shipping\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RoyalMail\\SDK\\V3Shipping\\Test\\": "tests/"
    }
  },
  "scripts": {
    "generate": "vendor/bin/openapi-code-generator generate -c ./code-generator-config.php -f ./\"Royal Mail API Shipping V3 (REST).yaml\""
  }
}
